                "type": "object",
                "properties": {
                    "subLayers": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202106alpha1SubLayer"
                        }
                    }
                }
            },
            "v202106alpha1ListSiteResponse": {
                "type": "object",
                "properties": {
                    "sites": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202106alpha1Site"
                        }
                    },
                    "invalidCount": {
                        "type": "integer",
                        "format": "int64"
                    }
                }
            },
            "v202106alpha1Site": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
                    "coordinate": {
                        "$ref": "#/components/schemas/typeLatLng"
                    },
                    "address": {
                        "$ref": "#/components/schemas/typePostalAddress"
                    },
                    "type": {
                        "$ref": "#/components/schemas/v202106alpha1SiteType"
                    },
                    "classification": {
                        "$ref": "#/components/schemas/v202106alpha1SiteIpClassification"
                    },
                    "architecture": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202106alpha1Layer"
                        }
                    }
                }
            },
            "v202106alpha1SiteIpClassification": {
                "type": "object",
                "properties": {
                    "infrastructureNetworks": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "userAccessNetworks": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "otherIps": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "title": "Site IP Classification"
            },
            "v202106alpha1SiteType": {
                "type": "string",
                "enum": [
                    "SITE_TYPE_UNSPECIFIED",
                    "SITE_TYPE_DATA_CENTER",
                    "SITE_TYPE_CLOUD",
                    "SITE_TYPE_BRANCH",
                    "SITE_TYPE_CONNECTIVITY",
                    "SITE_TYPE_CUSTOMER",
                    "SITE_TYPE_OTHER"
                ],
                "default": "SITE_TYPE_UNSPECIFIED"
            },
            "v202106alpha1SubLayer": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "deviceIds": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }
            },
            "v202106alpha1UpdateSiteRequest": {
                "type": "object",
                "properties": {
                    "site": {
                        "$ref": "#/components/schemas/v202106alpha1Site"
                    }
                }
            },
            "v202106alpha1UpdateSiteResponse": {
                "type": "object",
                "properties": {
                    "site": {
                        "$ref": "#/components/schemas/v202106alpha1Site"
                    }
                }
            }
        }
    }
}
