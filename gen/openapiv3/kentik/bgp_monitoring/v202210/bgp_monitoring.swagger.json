            "v202104RpkiStatus": {
                "type": "string",
                "enum": [
                    "RPKI_STATUS_UNSPECIFIED",
                    "RPKI_STATUS_NOT_FOUND",
                    "RPKI_STATUS_EXPLICIT_INVALID",
                    "RPKI_STATUS_INVALID_PREFIX",
                    "RPKI_STATUS_INVALID",
                    "RPKI_STATUS_VALID",
                    "RPKI_STATUS_ERROR"
