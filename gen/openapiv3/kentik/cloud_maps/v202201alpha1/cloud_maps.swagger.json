{
    "openapi": "3.0.0",
    "info": {
        "title": "Cloud Maps API",
        "version": "202201alpha1",
        "contact": {
            "name": "Kentik API Engineering"
        }
    },
    "tags": [
        {
            "name": "CloudMapsService"
        }
    ],
    "paths": {
        "/cloud_maps/v202201alpha1/ingest/aws": {
            "post": {
                "summary": "Collect given AWS metadata.",
                "description": "Provides location at with the API client may store AWS cloud map",
                "operationId": "CollectAwsMetadata",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202201alpha1ProvideAwsMetadataStorageResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202201alpha1ProvideAwsMetadataStorageRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "CloudMapsService"
                ]
            }
        }
    },
    "security": [
        {
            "email": [],
            "token": []
        }
    ],
    "externalDocs": {
        "description": "More about Kentik APIs",
        "url": "https://docs.kentik.com/api"
    },
    "components": {
        "securitySchemes": {
            "email": {
                "type": "apiKey",
                "name": "X-CH-Auth-Email",
                "in": "header"
            },
            "token": {
                "type": "apiKey",
                "name": "X-CH-Auth-API-Token",
                "in": "header"
            }
        },
        "schemas": {
            "protobufAny": {
                "type": "object",
                "properties": {
                    "typeUrl": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string",
                        "format": "byte"
                    }
                }
            },
            "rpcStatus": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "type": "string"
                    },
                    "details": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/protobufAny"
                        }
                    }
                }
            },
            "v202201alpha1ProvideAwsMetadataStorageRequest": {
                "type": "object",
                "properties": {
                    "version": {
                        "type": "string",
                        "title": "only \"aws-1.0\" literal for now"
                    },
                    "sourceAwsAccountId": {
                        "type": "string",
                        "title": "12-digit AWS account ID"
                    },
                    "sourceAwsRegion": {
                        "type": "string",
                        "title": "AWS region code"
                    }
                }
            },
            "v202201alpha1ProvideAwsMetadataStorageResponse": {
                "type": "object",
                "properties": {
                    "targetUrl": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
