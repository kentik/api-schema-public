{
    "openapi": "3.0.0",
    "info": {
        "title": "Mitigation Methods API",
        "description": "# Overview\nThe Alert Mitigation Methods API provides programmatic access to Kentik's capabilities of configuring and managing mitigation methods that define the specific parameters used for network infrastructure mitigations.",
        "version": "v202505",
        "contact": {
            "name": "Kentik API Engineering",
            "url": "https://github.com/kentik/api-schema-public"
        }
    },
    "tags": [
        {
            "name": "MitigationMethodsService"
        }
    ],
    "paths": {
        "/v202505/mitigations/methods": {
            "get": {
                "summary": "List Mitigation Methods",
                "description": "Returns a list of mitigation methods.",
                "operationId": "List",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202505MitigationMethodsServiceListResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "pagination.limit",
                        "description": "limit is how many items to return at most",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "uint64"
                        }
                    },
                    {
                        "name": "pagination.offset",
                        "description": "offset is the offset into the list of items",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "uint64"
                        }
                    },
                    {
                        "name": "pagination.includeTotalCount",
                        "description": "include_total_count is a flag to enforce API service to include the total count of items in the response,\nwhich in some cases may be very expensive to fetch",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    },
                    {
                        "name": "filters.methodIds",
                        "description": "The IDs of the mitigation methods to list.",
                        "in": "query",
                        "required": false,
                        "explode": true,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    {
                        "name": "filters.platformTypes",
                        "description": "The platform types the mitigation methods are associated with.\n\n - MITIGATION_PLATFORM_TYPE_A10: A10 mitigation platform\n - MITIGATION_PLATFORM_TYPE_FLOWSPEC: FlowSpec mitigation platform\n - MITIGATION_PLATFORM_TYPE_RADWARE: Radware mitigation platform\n - MITIGATION_PLATFORM_TYPE_RTBH: RTBH mitigation platform\n - MITIGATION_PLATFORM_TYPE_CFMT: CFMT mitigation platform\n - MITIGATION_PLATFORM_TYPE_ADAPTIVE_FLOWSPEC: Adaptive FlowSpec mitigation platform",
                        "in": "query",
                        "required": false,
                        "explode": true,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "enum": [
                                    "MITIGATION_PLATFORM_TYPE_UNSPECIFIED",
                                    "MITIGATION_PLATFORM_TYPE_A10",
                                    "MITIGATION_PLATFORM_TYPE_FLOWSPEC",
                                    "MITIGATION_PLATFORM_TYPE_RADWARE",
                                    "MITIGATION_PLATFORM_TYPE_RTBH",
                                    "MITIGATION_PLATFORM_TYPE_CFMT",
                                    "MITIGATION_PLATFORM_TYPE_ADAPTIVE_FLOWSPEC"
                                ]
                            }
                        }
                    },
                    {
                        "name": "filters.createdAt.start",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "name": "filters.createdAt.end",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "name": "filters.modifiedAt.start",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "name": "filters.modifiedAt.end",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    }
                ],
                "tags": [
                    "MitigationMethodsService"
                ]
            }
        },
        "/v202505/mitigations/methods/{id}": {
            "get": {
                "summary": "Get Mitigation Method",
                "description": "Returns a mitigation method.",
                "operationId": "Get",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202505MitigationMethodsServiceGetResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "id",
                        "description": "The ID of the mitigation method to get.",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "tags": [
                    "MitigationMethodsService"
                ]
            }
        }
    },
    "security": [
        {
            "email": [],
            "token": []
        }
    ],
    "externalDocs": {
        "description": "Kentik Alerts documentation",
        "url": "https://kb.kentik.com/docs/alerting"
    },
    "components": {
        "securitySchemes": {
            "email": {
                "type": "apiKey",
                "name": "X-CH-Auth-Email",
                "in": "header"
            },
            "token": {
                "type": "apiKey",
                "name": "X-CH-Auth-API-Token",
                "in": "header"
            }
        },
        "schemas": {
            "protobufAny": {
                "type": "object",
                "properties": {
                    "@type": {
                        "type": "string"
                    }
                },
                "additionalProperties": {}
            },
            "rpcStatus": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "type": "string"
                    },
                    "details": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/protobufAny"
                        }
                    }
                }
            },
            "typesv202506PaginationConfig": {
                "type": "object",
                "properties": {
                    "limit": {
                        "type": "string",
                        "format": "uint64",
                        "title": "limit is how many items to return at most"
                    },
                    "offset": {
                        "type": "string",
                        "format": "uint64",
                        "title": "offset is the offset into the list of items"
                    },
                    "includeTotalCount": {
                        "type": "boolean",
                        "title": "include_total_count is a flag to enforce API service to include the total count of items in the response,\nwhich in some cases may be very expensive to fetch"
                    }
                }
            },
            "typesv202506PaginationInfo": {
                "type": "object",
                "properties": {
                    "totalCount": {
                        "type": "string",
                        "format": "uint64",
                        "description": "total_count is the total number of items in the list.\nIt may be provided optionally if include_total_count is set to false.\nIt will be provided always if include_total_count is set to true."
                    },
                    "hasMoreResults": {
                        "type": "boolean",
                        "title": "has_more_results is a flag to indicate if there are more results to fetch\nIt may be provided instead of total_count if include_total_count is set to false and it would be very expensive to fetch the total count"
                    }
                }
            },
            "v202303TimeRange": {
                "type": "object",
                "properties": {
                    "start": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "end": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            },
            "v202505MitigationMethod": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "The ID of the mitigation method.",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "description": "The name of the mitigation method.",
                        "readOnly": true
                    },
                    "description": {
                        "type": "string",
                        "description": "The description of the mitigation method.",
                        "readOnly": true
                    },
                    "ackRequired": {
                        "type": "boolean",
                        "description": "Whether acknowledgment is required before applying this mitigation method.",
                        "readOnly": true
                    },
                    "excludedIpCidrs": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "The IP/CIDR ranges that are excluded from this mitigation method.",
                        "readOnly": true
                    },
                    "gracePeriod": {
                        "type": "string",
                        "description": "The grace period before the mitigation method is applied.",
                        "readOnly": true
                    },
                    "type": {
                        "$ref": "#/components/schemas/v202505MitigationPlatformType"
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "The time when the mitigation method was created.",
                        "readOnly": true
                    },
                    "modifiedAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "The time when the mitigation method was last modified.",
                        "readOnly": true
                    },
                    "notifications": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202505NotificationChannelAssociation"
                        },
                        "description": "The notification channel associations for this mitigation method.",
                        "readOnly": true
                    }
                }
            },
            "v202505MitigationMethodsFilters": {
                "type": "object",
                "properties": {
                    "methodIds": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "The IDs of the mitigation methods to list."
                    },
                    "platformTypes": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202505MitigationPlatformType"
                        },
                        "description": "The platform types the mitigation methods are associated with."
                    },
                    "createdAt": {
                        "$ref": "#/components/schemas/v202303TimeRange"
                    },
                    "modifiedAt": {
                        "$ref": "#/components/schemas/v202303TimeRange"
                    }
                }
            },
            "v202505MitigationMethodsServiceGetResponse": {
                "type": "object",
                "properties": {
                    "method": {
                        "$ref": "#/components/schemas/v202505MitigationMethod"
                    }
                }
            },
            "v202505MitigationMethodsServiceListResponse": {
                "type": "object",
                "properties": {
                    "pagination": {
                        "$ref": "#/components/schemas/typesv202506PaginationInfo"
                    },
                    "methods": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202505MitigationMethod"
                        },
                        "description": "The mitigation methods that were found matching the filters.",
                        "readOnly": true
                    }
                }
            },
            "v202505MitigationPlatformType": {
                "type": "string",
                "enum": [
                    "MITIGATION_PLATFORM_TYPE_UNSPECIFIED",
                    "MITIGATION_PLATFORM_TYPE_A10",
                    "MITIGATION_PLATFORM_TYPE_FLOWSPEC",
                    "MITIGATION_PLATFORM_TYPE_RADWARE",
                    "MITIGATION_PLATFORM_TYPE_RTBH",
                    "MITIGATION_PLATFORM_TYPE_CFMT",
                    "MITIGATION_PLATFORM_TYPE_ADAPTIVE_FLOWSPEC"
                ],
                "default": "MITIGATION_PLATFORM_TYPE_UNSPECIFIED",
                "title": "- MITIGATION_PLATFORM_TYPE_A10: A10 mitigation platform\n - MITIGATION_PLATFORM_TYPE_FLOWSPEC: FlowSpec mitigation platform\n - MITIGATION_PLATFORM_TYPE_RADWARE: Radware mitigation platform\n - MITIGATION_PLATFORM_TYPE_RTBH: RTBH mitigation platform\n - MITIGATION_PLATFORM_TYPE_CFMT: CFMT mitigation platform\n - MITIGATION_PLATFORM_TYPE_ADAPTIVE_FLOWSPEC: Adaptive FlowSpec mitigation platform"
            },
            "v202505NotificationChannelAssociation": {
                "type": "object",
                "properties": {
                    "channelId": {
                        "type": "string",
                        "description": "The ID of the notification channel."
                    }
                },
                "required": [
                    "channelId"
                ]
            }
        }
    }
}
