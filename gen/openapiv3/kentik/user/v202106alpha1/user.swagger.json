{
    "openapi": "3.0.0",
    "info": {
        "title": "User management API",
        "version": "202102alpha1",
        "contact": {
            "name": "Kentik API Engineering",
            "url": "https://github.com/kentik/api"
        }
    },
    "tags": [
        {
            "name": "UserService"
        }
    ],
    "paths": {
        "/user/v202106alpha1/users": {
            "get": {
                "summary": "List users.",
                "description": "Returns a list of user accounts.",
                "operationId": "UserList",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202106alpha1ListUserResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "UserService"
                ]
            },
            "post": {
                "summary": "Create a user account.",
                "description": "Create a user account from request. returns created account.",
                "operationId": "UserCreate",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202106alpha1CreateUserResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202106alpha1CreateUserRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "UserService"
                ]
            }
        },
        "/user/v202106alpha1/users/{id}": {
            "get": {
                "summary": "Get a user account.",
                "description": "Returns information about user account specified with ID.",
                "operationId": "UserGet",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202106alpha1GetUserResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "tags": [
                    "UserService"
                ]
            },
            "delete": {
                "summary": "Delete a user account.",
                "description": "Deletes the user account specified with id.",
                "operationId": "UserDelete",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202106alpha1DeleteUserResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "tags": [
                    "UserService"
                ]
            },
            "put": {
                "summary": "Update a user account.",
                "description": "Replaces the entire user account attributes specified with id.",
                "operationId": "UserUpdate",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202106alpha1UpdateUserResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202106alpha1UpdateUserRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "UserService"
                ]
            }
        }
    },
    "security": [
        {
            "email": [],
            "token": []
        }
    ],
    "externalDocs": {
        "description": "More about Kentik APIs",
        "url": "https://docs.kentik.com/api"
    },
    "components": {
        "securitySchemes": {
            "email": {
                "type": "apiKey",
                "name": "X-CH-Auth-Email",
                "in": "header"
            },
            "token": {
                "type": "apiKey",
                "name": "X-CH-Auth-API-Token",
                "in": "header"
            }
        },
        "schemas": {
            "protobufAny": {
                "type": "object",
                "properties": {
                    "typeUrl": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string",
                        "format": "byte"
                    }
                }
            },
            "rpcStatus": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "type": "string"
                    },
                    "details": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/protobufAny"
                        }
                    }
                }
            },
            "v202106alpha1CreateUserRequest": {
                "type": "object",
                "properties": {
                    "user": {
                        "$ref": "#/components/schemas/v202106alpha1User"
                    }
                }
            },
            "v202106alpha1CreateUserResponse": {
                "type": "object",
                "properties": {
                    "user": {
                        "$ref": "#/components/schemas/v202106alpha1User"
                    }
                }
            },
            "v202106alpha1DeleteUserResponse": {
                "type": "object"
            },
            "v202106alpha1GetUserResponse": {
                "type": "object",
                "properties": {
                    "user": {
                        "$ref": "#/components/schemas/v202106alpha1User"
                    }
                }
            },
            "v202106alpha1ListUserResponse": {
                "type": "object",
                "properties": {
                    "users": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202106alpha1User"
                        }
                    },
                    "invalidCount": {
                        "type": "integer",
                        "format": "int64"
                    }
                }
            },
            "v202106alpha1Role": {
                "type": "string",
                "enum": [
                    "ROLE_UNSPECIFIED",
                    "ROLE_MEMBER",
                    "ROLE_ADMINISTRATOR",
                    "ROLE_SUPER_ADMINISTRATOR"
                ],
                "default": "ROLE_UNSPECIFIED"
            },
            "v202106alpha1UpdateUserRequest": {
                "type": "object",
                "properties": {
                    "user": {
                        "$ref": "#/components/schemas/v202106alpha1User"
                    }
                }
            },
            "v202106alpha1UpdateUserResponse": {
                "type": "object",
                "properties": {
                    "user": {
                        "$ref": "#/components/schemas/v202106alpha1User"
                    }
                }
            },
            "v202106alpha1User": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "userEmail": {
                        "type": "string"
                    },
                    "username": {
                        "type": "string"
                    },
                    "userFullName": {
                        "type": "string"
                    },
                    "role": {
                        "$ref": "#/components/schemas/v202106alpha1Role"
                    },
                    "lastLogin": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "createdDate": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updatedDate": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            }
        }
    }
}
