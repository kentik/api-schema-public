{
    "openapi": "3.0.0",
    "info": {
        "title": "KMI Configuration API",
        "description": "# Overview\nThe KMI Configuration API provides programmatic access to KMI Rankings.\n",
        "version": "v202212",
        "contact": {
            "name": "Kentik API Engineering",
            "url": "https://github.com/kentik/api-schema-public"
        }
    },
    "tags": [
        {
            "name": "KmiService"
        }
    ],
    "paths": {
        "/kmi/v202212/market/{marketId}/network/{asn}/{type}": {
            "post": {
                "summary": "List metadata and list of customers, providers, and peers for ASN.",
                "description": "Returns metadata and list of customers, providers, and peers for ASN.",
                "operationId": "GetASNDetails",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202212GetASNDetailsResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "marketId",
                        "description": "Unique Market identifier (as provided by the ListMarkets RPC)",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "asn",
                        "description": "Autonomous System Number",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "type",
                        "description": "Type of the requested ASN ('all', 'customer', 'provider', 'peer'). Defaults to 'all'.",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202212GetASNDetailsRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "KmiService"
                ]
            }
        },
        "/kmi/v202212/market/{marketId}/rankings/{rankType}": {
            "post": {
                "summary": "List all KMI rankings.",
                "description": "Returns list of KMI rankings.",
                "operationId": "GetRankings",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202212GetRankingsResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "marketId",
                        "description": "Unique Market identifier (as provided by the ListMarkets RPC)",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "rankType",
                        "description": "Type of the requested ranking ('customer_base', 'customer_base_retail', 'customer_base_wholesome', 'customer_base_backbone', 'customer_growth', 'peering_base'). Defaults to 'customer_base'.",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202212GetRankingsRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "KmiService"
                ]
            }
        },
        "/kmi/v202212/markets": {
            "get": {
                "summary": "List all markets for KMI.",
                "description": "Returns list of markets for KMI.",
                "operationId": "ListMarkets",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202212ListMarketsResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "KmiService"
                ]
            }
        }
    },
    "security": [
        {
            "email": [],
            "token": []
        }
    ],
    "externalDocs": {
        "description": "General information about Kentik APIs",
        "url": "https://kb.kentik.com/v0/Ab09.htm#Ab09-APIs_Overview"
    },
    "components": {
        "securitySchemes": {
            "email": {
                "type": "apiKey",
                "name": "X-CH-Auth-Email",
                "in": "header"
            },
            "token": {
                "type": "apiKey",
                "name": "X-CH-Auth-API-Token",
                "in": "header"
            }
        },
        "schemas": {
            "protobufAny": {
                "type": "object",
                "properties": {
                    "typeUrl": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string",
                        "format": "byte"
                    }
                }
            },
            "rpcStatus": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "type": "string"
                    },
                    "details": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/protobufAny"
                        }
                    }
                }
            },
            "v202212ASNDetails": {
                "type": "object",
                "properties": {
                    "asn": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Autonomous System Number",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "description": "Name",
                        "readOnly": true
                    },
                    "countryName": {
                        "type": "string",
                        "description": "Country Name",
                        "readOnly": true
                    },
                    "customers": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202212CustomerProvider"
                        },
                        "description": "Customers",
                        "readOnly": true
                    },
                    "providers": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202212CustomerProvider"
                        },
                        "description": "Providers",
                        "readOnly": true
                    },
                    "peers": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202212Peer"
                        },
                        "description": "Peers",
                        "readOnly": true
                    }
                },
                "title": "ASNDetails"
            },
            "v202212CustomerProvider": {
                "type": "object",
                "properties": {
                    "asn": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Autonomous System Number",
                        "readOnly": true
                    },
                    "edgeType": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Edge Type",
                        "readOnly": true
                    },
                    "geoName": {
                        "type": "string",
                        "description": "Geo Name (e.g. 'US' when Unique Market identifier is 'cc$US, noting 'US' is the country code within the 'cc' scope)",
                        "readOnly": true
                    },
                    "geoScope": {
                        "type": "string",
                        "description": "Geo Scope (e.g. 'cc' when Unique Market identifier is 'cc$US', noting the scope for 'US' is 'cc' or country code)",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "description": "Name",
                        "readOnly": true
                    },
                    "score": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Score",
                        "readOnly": true
                    },
                    "singlehomedCustomer": {
                        "type": "boolean",
                        "description": "Singlehomed customer",
                        "readOnly": true
                    },
                    "mutualCustomer": {
                        "type": "boolean",
                        "description": "Mutual customer",
                        "readOnly": true
                    },
                    "mutualProvider": {
                        "type": "boolean",
                        "description": "Mutual provider",
                        "readOnly": true
                    }
                },
                "title": "CustomerProvider"
            },
            "v202212GetASNDetailsRequest": {
                "type": "object",
                "properties": {
                    "marketId": {
                        "type": "string",
                        "description": "Unique Market identifier (as provided by the ListMarkets RPC)",
                        "required": [
                            "market_id"
                        ]
                    },
                    "asn": {
                        "type": "string",
                        "description": "Autonomous System Number",
                        "required": [
                            "asn"
                        ]
                    },
                    "ip": {
                        "type": "string",
                        "description": "IP Address Family ('v4' or 'v6'). Defaults to 'v4'."
                    },
                    "type": {
                        "type": "string",
                        "description": "Type of the requested ASN ('all', 'customer', 'provider', 'peer'). Defaults to 'all'."
                    },
                    "mutualProvider": {
                        "type": "string",
                        "description": "Filter by mutual provider ('all', 'only', 'exclude'). Defaults to 'all'."
                    },
                    "mutualCustomer": {
                        "type": "string",
                        "description": "Filter by mutual customer ('all', 'only', 'exclude'). Defaults to 'all'."
                    },
                    "singlehomedCustomer": {
                        "type": "string",
                        "description": "Filter by singlehomed customer ('all', 'only', 'exclude'). Defaults to 'all'."
                    }
                },
                "title": "GetASNDetailsRequest",
                "required": [
                    "marketId",
                    "asn"
                ]
            },
            "v202212GetASNDetailsResponse": {
                "type": "object",
                "properties": {
                    "asnDetails": {
                        "$ref": "#/components/schemas/v202212ASNDetails"
                    }
                },
                "title": "GetASNDetailsResponse"
            },
            "v202212GetRankingsRequest": {
                "type": "object",
                "properties": {
                    "marketId": {
                        "type": "string",
                        "description": "Unique Market identifier (as provided by the ListMarkets RPC)",
                        "required": [
                            "market_id"
                        ]
                    },
                    "rankType": {
                        "type": "string",
                        "description": "Type of the requested ranking ('customer_base', 'customer_base_retail', 'customer_base_wholesome', 'customer_base_backbone', 'customer_growth', 'peering_base'). Defaults to 'customer_base'."
                    },
                    "ip": {
                        "type": "string",
                        "description": "IP Address Family ('v4' or 'v6') of requested ranking. Defaults to 'v4'."
                    },
                    "limit": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Maximum number of entries returned. (Default: 600)."
                    }
                },
                "title": "GetRankingsRequest",
                "required": [
                    "marketId"
                ]
            },
            "v202212GetRankingsResponse": {
                "type": "object",
                "properties": {
                    "rankings": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202212Ranking"
                        },
                        "description": "List of rankings"
                    },
                    "invalidCount": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Number of invalid entries encountered while collecting data"
                    }
                },
                "title": "GetRankingsResponse"
            },
            "v202212ListMarketsResponse": {
                "type": "object",
                "properties": {
                    "markets": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202212Market"
                        },
                        "description": "Markets",
                        "readOnly": true
                    }
                },
                "title": "ListMarketsResponse"
            },
            "v202212Market": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "Unique Market identifier",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "description": "Market Name",
                        "readOnly": true
                    },
                    "geoScope": {
                        "type": "string",
                        "description": "Geo Scope (e.g. 'cc' when Unique Market identifier is 'cc$US', noting the scope for 'US' is 'cc' or country code)",
                        "readOnly": true
                    },
                    "geoName": {
                        "type": "string",
                        "description": "Geo Name (e.g. 'US' when Unique Market identifier is 'cc$US, noting 'US' is the country code within the 'cc' scope)",
                        "readOnly": true
                    },
                    "childList": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "List of child markets (e.g. Unique Market identifier 'cc$US' contains child market 'us_maj_reg$USMW')",
                        "readOnly": true
                    },
                    "parentList": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "List of parent markets (e.g. Unique Market identifier 'us_maj_reg$USMW' contains parent market 'cc$US')",
                        "readOnly": true
                    }
                },
                "title": "Market"
            },
            "v202212Peer": {
                "type": "object",
                "properties": {
                    "asn": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Autonomous System Number",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "description": "Name",
                        "readOnly": true
                    },
                    "pfxCount": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Prefix Count",
                        "readOnly": true
                    }
                },
                "title": "Peer"
            },
            "v202212Ranking": {
                "type": "object",
                "properties": {
                    "asn": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Autonomous System Number",
                        "readOnly": true
                    },
                    "geoScope": {
                        "type": "string",
                        "description": "Geo Scope (e.g. 'cc' when Unique Market identifier is 'cc$US', noting the scope for 'US' is 'cc' or country code)",
                        "readOnly": true
                    },
                    "geoName": {
                        "type": "string",
                        "description": "Geo Name (e.g. 'US' when Unique Market identifier is 'cc$US, noting 'US' is the country code within the 'cc' scope)",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "description": "Name",
                        "readOnly": true
                    },
                    "rank": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Rank",
                        "readOnly": true
                    },
                    "rankChange": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Rank Change",
                        "readOnly": true
                    },
                    "score": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Score",
                        "readOnly": true
                    },
                    "scoreChange": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Score Change",
                        "readOnly": true
                    }
                },
                "title": "Ranking"
            }
        }
    }
}
