                    },
                    "score": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Score of the Autonomous System",
                        "readOnly": true
                    },
                    "scoreChange": {
                        "type": "integer",
                        "format": "int64",
                        "description": "Score Change of the Autonomous System",
                        "readOnly": true
                    }
                },
                "title": "Ranking"
            }
        }
    }
}
