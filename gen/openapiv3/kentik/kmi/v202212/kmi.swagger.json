                        "readOnly": true
                    }
                },
                "title": "ListMarketsResponse"
            },
            "v202212Market": {
                "type": "object",
                "properties": {
                    "marketId": {
                        "type": "string",
                        "description": "Unique Geo Market identifier",
                        "readOnly": true
                    },
                    "name": {
                        "type": "string",
                        "description": "Geo Market Name",
                        "readOnly": true
                    }
                },
                "title": "Market"
