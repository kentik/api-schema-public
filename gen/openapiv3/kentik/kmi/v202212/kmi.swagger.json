                "title": "GetASNDetailsResponse"
            },
            "v202212GetASNInsightsResponse": {
                "type": "object",
                "properties": {
                    "insights": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202212Insight"
