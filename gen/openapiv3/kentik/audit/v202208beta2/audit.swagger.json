{
    "openapi": "3.0.0",
    "info": {
        "title": "Audit API",
        "version": "202208beta1",
        "contact": {
            "name": "Kentik API Engineering",
            "url": "https://github.com/kentik/api"
        }
    },
    "tags": [
        {
            "name": "AuditService"
        }
    ],
    "paths": {
        "/audit/v202208beta2/events": {
            "get": {
                "summary": "List Audit Events.",
                "description": "Returns a list of audit events.",
                "operationId": "AuditEventList",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202208beta2ListAuditEventsResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "startTime",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "name": "endTime",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "name": "offset",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "uint64",
                            "default": "0"
                        }
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "uint64",
                            "default": "100"
                        }
                    }
                ],
                "tags": [
                    "AuditService"
                ]
            },
            "post": {
                "summary": "Create one ore more audit events.",
                "description": "Create one ore more audit events.",
                "operationId": "CreateAuditEvents",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202208beta2CreateAuditEventsResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202208beta2CreateAuditEventsRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "AuditService"
                ]
            }
        },
        "/audit/v202208beta2/events/{id}/{ctime}": {
            "get": {
                "summary": "Get an Audit Event",
                "description": "Return a specific audit event.",
                "operationId": "GetAuditEvent",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202208beta2GetAuditEventResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "ctime",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    }
                ],
                "tags": [
                    "AuditService"
                ]
            }
        }
    },
    "security": [
        {
            "email": [],
            "token": []
        }
    ],
    "externalDocs": {
        "description": "More about Kentik APIs",
        "url": "https://docs.kentik.com/api"
    },
    "components": {
        "securitySchemes": {
            "email": {
                "type": "apiKey",
                "name": "X-CH-Auth-Email",
                "in": "header"
            },
            "token": {
                "type": "apiKey",
                "name": "X-CH-Auth-API-Token",
                "in": "header"
            }
        },
        "schemas": {
            "protobufAny": {
                "type": "object",
                "properties": {
                    "typeUrl": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string",
                        "format": "byte"
                    }
                }
            },
            "rpcStatus": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "type": "string"
                    },
                    "details": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/protobufAny"
                        }
                    }
                }
            },
            "v202208beta2AuditEvent": {
                "type": "object",
                "properties": {
                    "userId": {
                        "type": "string",
                        "title": "ID of the user (uid) that produced the event"
                    },
                    "companyId": {
                        "type": "string",
                        "title": "ID of the company (cid) that produced the event"
                    },
                    "serviceName": {
                        "type": "string",
                        "description": "Name of the service that generated the event."
                    },
                    "eventId": {
                        "type": "string",
                        "title": "Unique identifier of the event. EventID and ServiceName are used to identify an event uniquely"
                    },
                    "ctime": {
                        "type": "string",
                        "format": "date-time",
                        "description": "When the event was produced. Required."
                    },
                    "eventPayload": {
                        "type": "string",
                        "title": "Event payload as a JSON string"
                    },
                    "correlationId": {
                        "type": "string",
                        "title": "Idenfier for events generated by the same root transaction. For instrumented services, this is usually the trace ID"
                    },
                    "id": {
                        "type": "string",
                        "format": "uint64",
                        "title": "ID of the event in the database (read-only)"
                    },
                    "request": {
                        "$ref": "#/components/schemas/v202208beta2RequestEvent"
                    }
                }
            },
            "v202208beta2CreateAuditEventsRequest": {
                "type": "object",
                "properties": {
                    "events": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202208beta2AuditEvent"
                        }
                    }
                }
            },
            "v202208beta2CreateAuditEventsResponse": {
                "type": "object"
            },
            "v202208beta2GetAuditEventResponse": {
                "type": "object",
                "properties": {
                    "event": {
                        "$ref": "#/components/schemas/v202208beta2AuditEvent"
                    }
                }
            },
            "v202208beta2ListAuditEventsResponse": {
                "type": "object",
                "properties": {
                    "events": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202208beta2AuditEvent"
                        }
                    }
                }
            },
            "v202208beta2RequestEvent": {
                "type": "object",
                "properties": {
                    "requestMethod": {
                        "type": "string"
                    },
                    "requestPath": {
                        "type": "string"
                    },
                    "clientAddress": {
                        "type": "string",
                        "title": "IP address of the client that made the request. Example: 75.2.60.5"
                    },
                    "responseCode": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "requestMetadata": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "string"
                        }
                    },
                    "responseMetadata": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "string"
                        }
                    },
                    "requestProtocol": {
                        "$ref": "#/components/schemas/v202208beta2RequestProtocol"
                    }
                },
                "title": "RequestEvent describes an event generated by an REST or a gRPC request"
            },
            "v202208beta2RequestProtocol": {
                "type": "string",
                "enum": [
                    "REQUEST_PROTOCOL_UNSPECIFIED",
                    "REQUEST_PROTOCOL_REST",
                    "REQUEST_PROTOCOL_GRPC"
                ],
                "default": "REQUEST_PROTOCOL_UNSPECIFIED"
            }
        }
    }
}
