                    },
                    "metadata": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "string"
                        },
                        "title": "generic bag of metadata about the event"
                    }
                }
            },
            "v202601GetAuditEventResponse": {
                "type": "object",
                "properties": {
                    "event": {
                        "$ref": "#/components/schemas/v202601AuditEvent"
                    }
                }
            },
            "v202601ListAuditEventsResponse": {
                "type": "object",
                "properties": {
                    "events": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202601AuditEvent"
                        }
                    }
                }
            }
        }
    }
