{
    "openapi": "3.0.0",
    "info": {
        "title": "Label API",
        "version": "202203alpha1",
        "contact": {
            "name": "Kentik API Engineering"
        }
    },
    "tags": [
        {
            "name": "LabelService"
        }
    ],
    "paths": {
        "/label/v202203alpha1/labels": {
            "get": {
                "summary": "Fetch Search Labels",
                "description": "Return list of labels matches search critera.",
                "operationId": "ListLabel",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202203alpha1ListLabelsResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "LabelService"
                ]
            },
            "post": {
                "summary": "Create a label.",
                "description": "Create a label from request. returns created.",
                "operationId": "LabelCreate",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202203alpha1CreateLabelResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202203alpha1CreateLabelRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "LabelService"
                ]
            }
        },
        "/label/v202203alpha1/labels/{id}": {
            "delete": {
                "summary": "Delete a label.",
                "description": "Deletes the label specified with id.",
                "operationId": "LabelDelete",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202203alpha1DeleteLabelResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "id",
                        "description": "ID of label.",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "tags": [
                    "LabelService"
                ]
            },
            "post": {
                "summary": "Update a label.",
                "description": "Replaces the entire label attributes specified with id.",
                "operationId": "LabelUpdate",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202203alpha1UpdateLabelResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202203alpha1UpdateLabelRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "LabelService"
                ]
            }
        }
    },
    "security": [
        {
            "email": [],
            "token": []
        }
    ],
