            "labelv202203alpha1Label": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "color": {
                        "type": "string",
                        "title": "hex color code value"
                    },
                    "cdate": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Create timestamp."
                    },
                    "edate": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Update timestamp."
                    }
                }
            },
            "protobufAny": {
                "type": "object",
                "properties": {
                    "typeUrl": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string",
                        "format": "byte"
                    }
                }
            },
            "rpcStatus": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "type": "string"
                    },
                    "details": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/protobufAny"
                        }
                    }
                }
            },
            "v202203alpha1CreateLabelRequest": {
                "type": "object",
                "properties": {
                    "label": {
                        "$ref": "#/components/schemas/labelv202203alpha1Label"
                    }
                }
            },
            "v202203alpha1CreateLabelResponse": {
                "type": "object",
                "properties": {
                    "label": {
                        "$ref": "#/components/schemas/labelv202203alpha1Label"
                    }
                }
            },
            "v202203alpha1DeleteLabelResponse": {
                "type": "object"
            },
            "v202203alpha1ListLabelsResponse": {
                "type": "object",
                "properties": {
                    "labels": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/labelv202203alpha1Label"
                        }
                    },
                    "invalidCount": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "v202203alpha1UpdateLabelRequest": {
                "type": "object",
                "properties": {
                    "label": {
                        "$ref": "#/components/schemas/labelv202203alpha1Label"
                    }
                }
            },
            "v202203alpha1UpdateLabelResponse": {
                "type": "object",
                "properties": {
                    "label": {
                        "$ref": "#/components/schemas/labelv202203alpha1Label"
                    }
                }
            }
        }
