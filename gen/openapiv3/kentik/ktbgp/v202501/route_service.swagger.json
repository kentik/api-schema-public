                "type": "string",
                "enum": [
                    "ADVERT_STATUS_UNSPECIFIED",
                    "ADVERT_STATUS_ANNOUNCED",
                    "ADVERT_STATUS_WITHDRAWN",
                    "ADVERT_STATUS_NO_SESSION",
                    "ADVERT_STATUS_FAILED"
                ],
                "default": "ADVERT_STATUS_UNSPECIFIED",
                "title": "- ADVERT_STATUS_NO_SESSION: when we cannot find the chf-bgp instance\n - ADVERT_STATUS_FAILED: when we failed to parse or send the update"
            },
            "v202501BitwiseOp": {
                "type": "string",
