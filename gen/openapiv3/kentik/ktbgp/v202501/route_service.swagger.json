{
    "openapi": "3.0.0",
    "info": {
        "title": "kentik/ktbgp/v202501/route_service.proto",
        "version": "version not set"
    },
    "tags": [
        {
            "name": "RouteService"
        }
    ],
    "paths": {
        "/routes/announce": {
            "post": {
                "summary": "Announce a BGP route to a specified set of devices",
                "operationId": "RouteService_Announce",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202501RouteServiceAnnounceResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202501RouteServiceAnnounceRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "RouteService"
                ]
            }
        },
        "/routes/list": {
            "post": {
                "summary": "List active BGP updates for a specified set of devices",
                "operationId": "RouteService_List",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202501RouteServiceListResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202501RouteServiceListRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "RouteService"
                ]
            }
        },
        "/routes/withdraw": {
            "post": {
                "summary": "Withdraw active BGP updates from devices",
                "operationId": "RouteService_Withdraw",
                "responses": {
                    "200": {
                        "description": "A successful response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/v202501RouteServiceWithdrawResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "An unexpected error response.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/rpcStatus"
                                }
                            }
                        }
                    }
                },
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/v202501RouteServiceWithdrawRequest"
                            }
                        }
                    },
                    "required": true
                },
                "tags": [
                    "RouteService"
                ]
            }
        }
    },
