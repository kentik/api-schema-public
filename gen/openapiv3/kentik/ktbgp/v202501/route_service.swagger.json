                        "$ref": "#/components/schemas/v202501FlowspecActionDiscard"
                    },
                    "rateBytes": {
                        "$ref": "#/components/schemas/v202501FlowspecActionTrafficRateBytes"
                    },
                    "markDscp": {
                        "$ref": "#/components/schemas/v202501FlowspecActionMarkDSCP"
                    },
                    "rtRedirect": {
