                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202501FlowspecUpdate"
                        }
                    },
                    "rtbh": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/v202501RTBHUpdate"
                        }
                    }
                }
            },
