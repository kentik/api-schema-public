                        "$ref": "#/components/schemas/v202501FlowspecActionAccept"
                    },
                    "actionType": {
                        "$ref": "#/components/schemas/v202501FlowspecActionTerminalSample"
                    },
                    "rtCopy": {
                        "$ref": "#/components/schemas/v202501FlowspecActionIPNextHopCopy"
