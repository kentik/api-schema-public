              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v202108alpha1CreateInterfaceRequest"
            }
          }
        ],
        "tags": [
          "InterfaceService"
        ]
      }
    },
    "/interface/v202108alpha1/interfaces/{id}": {
      "get": {
        "summary": "Get a interface.",
        "description": "Returns information about a interface specified with ID.",
        "operationId": "InterfaceGet",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v202108alpha1GetInterfaceResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "description": "ID of interface.",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "InterfaceService"
        ]
      },
      "delete": {
        "summary": "Delete a interface.",
        "description": "Deletes the interface specified with id.",
        "operationId": "InterfaceDelete",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v202108alpha1DeleteInterfaceResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "description": "ID of interface.",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "InterfaceService"
        ]
      },
      "put": {
        "summary": "Update a interface.",
        "description": "Replaces the entire interface attributes specified with id.",
        "operationId": "InterfaceUpdate",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v202108alpha1UpdateInterfaceResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "description": "ID of interface.",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v202108alpha1UpdateInterfaceRequest"
            }
          }
        ],
        "tags": [
          "InterfaceService"
        ]
      }
    },
    "/interface/v202108alpha1/manual_classify": {
      "post": {
        "summary": "Manual Classify Interface",
        "description": "Manually set interface(s) classification.",
        "operationId": "ManualClassify",
        "responses": {
          "200": {
            "description": "A successful response.",
