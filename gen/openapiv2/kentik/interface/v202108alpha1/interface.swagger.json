          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v202108alpha1ManualClassifyRequest"
            }
          }
        ],
        "tags": [
          "InterfaceService"
        ]
      }
    }
  },
